{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "https://quickdrive-9xxc.onrender.com/api",
    "REACT_APP_APP_NAME": "Project Management System",
    "REACT_APP_VERSION": "1.0.0"
  }
}
